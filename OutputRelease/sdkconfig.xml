<?xml version="1.0" encoding="UTF-8"?>

<!--媒体平台相关配置-->
<config xsi:noNamespaceSchemaLocation="SDKSchema.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" >
    <!--缓存队列设置-->
    <offlineCache>
        <length>0</length>
        <queueExpirationSecs>60</queueExpirationSecs>
        <!--发送超时时间-->
        <timeout>60</timeout>
    </offlineCache>

    <viewability>
        <!--viewability监测的时间间隔（ms）-->
        <intervalTime>100</intervalTime>
        <!--满足viewability可见区域占总区域的百分比-->
        <viewabilityFrame>50</viewabilityFrame>
        <!--满足普通viewability总时长（s）-->
        <viewabilityTime>1</viewabilityTime>
        <!--满足视频viewability总时长（s）-->
        <viewabilityVideoTime>2</viewabilityVideoTime>
        <!--当前广告位最大监测时长（s）-->
        <maxExpirationSecs>120</maxExpirationSecs>
        <!--当前广告位最大上报数量-->
        <maxAmount>20</maxAmount>
    </viewability>

    <companies>
        <company>

            <name>admaster</name>
            <!-- JS监测在线更新地址 http://developer.admaster.com.cn/docs/mma-sdk.min.js -->
            <jsurl></jsurl>
            <jsname></jsname>

            <domain>
                <!-- 此处需修改为第三方检测公司监测代码的 host 部分 -->
                <url>admaster.com.cn</url>
            </domain>
            <signature>
                <publicKey>Z83476Hel</publicKey>
                <paramKey>sign</paramKey>
            </signature>
            <switch>
                <isTrackLocation>false</isTrackLocation>
                <!-- 失效时间，单位秒 -->
                <offlineCacheExpiration>259200</offlineCacheExpiration>
                <!-- 可视化监测采集策略 0 = TrackPositionChanged 位置改变时记录,1 = TrackVisibleChanged 可视改变时记录-->
                <viewabilityTrackPolicy>0</viewabilityTrackPolicy>
                <encrypt>
                    <MAC>md5</MAC>
                    <IDA>md5</IDA>
                    <IMEI>md5</IMEI>
                    <ANDROIDID>raw</ANDROIDID>
                </encrypt>
            </switch>
            <config>
                <arguments>
                    <!--argument的必选和常用可选参数 key需确定-->
                    <!--必选函数-->
                    <argument>
                        <key>OS</key>
                        <value>0a</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>

                    <argument>
                        <key>TS</key>
                        <value>t</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>

                    <argument>
                        <key>MAC</key>
                        <value>n</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>

                    <argument>
                        <key>IDFA</key>
                        <value>z</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>

                    <argument>
                        <key>IDFAMD5</key>
                        <value>0j</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>

                    <argument>
                        <key>IMEI</key>
                        <value>0c</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>

                    <argument>
                        <key>ANDROIDID</key>
                        <value>0d</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>

                    <argument>
                        <key>WIFI</key>
                        <value>w</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>

                    <argument>
                        <key>AKEY</key>
                        <value>x</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>

                    <argument>
                        <key>ANAME</key>
                        <value>y</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <!--可选函数-->
                    <argument>
                        <key>SCWH</key>
                        <value>0f</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>

                    <argument>
                        <key>OPENUDID</key>
                        <value>o</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <argument>
                        <key>TERM</key>
                        <value>r</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>

                    <argument>
                        <key>OSVS</key>
                        <value>q</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>

                    <argument>
                        <key>LBS</key>
                        <value>l</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <argument>
                        <key>SDKVS</key>
                        <value>0l</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <argument>
                        <key>REDIRECTURL</key>
                        <value>u</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                </arguments>
                <events>
                    <event>
                        <!--<name>m1</name>-->
                        <key>start</key>
                        <value>m201</value>
                        <urlEncode>true</urlEncode>
                    </event>
                    <event>
                        <!--<name>e1</name>-->
                        <key>end</key>
                        <value>m203</value>
                        <urlEncode>true</urlEncode>
                    </event>
                </events>
                <!-- 广告位标识符 -->
                <Adplacement>
                    <argument>
                        <key>Adplacement</key>
                        <value>b</value>
                        <urlEncode>false</urlEncode>
                        <isRequired>false</isRequired>
                    </argument>
                </Adplacement>

                <viewabilityarguments>
                    <!-- 可视监测识别ID -->
                    <argument>
                        <key>ImpressionID</key>
                        <value>2g</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <!-- 采集属性 start -->
                    <!-- 可视视图采集轨迹数据 -->
                    <argument>
                        <key>AdviewabilityEvents</key>
                        <value>2j</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <!-- 可视视图采集点时间戳 -->
                    <argument>
                        <key>AdviewabilityTime</key>
                        <value>2t</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <!-- 可视视图尺寸 -->
                    <argument>
                        <key>AdviewabilityFrame</key>
                        <value>2k</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <!-- 视图可视区域坐标 -->
                    <argument>
                        <key>AdviewabilityPoint</key>
                        <value>2d</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <!-- 可视视图透明度 -->
                    <argument>
                        <key>AdviewabilityAlpha</key>
                        <value>2l</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <!-- 可视视图是否被隐藏 -->
                    <argument>
                        <key>AdviewabilityHide</key>
                        <value>2m</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <!-- 可视视图被覆盖率 -->
                    <argument>
                        <key>AdviewabilityCoverRate</key>
                        <value>2n</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <!-- 视图可视尺寸 -->
                    <argument>
                        <key>AdviewabilityShowFrame</key>
                        <value>2o</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <!-- 屏幕是否点亮 -->
                    <argument>
                        <key>AdviewabilityLight</key>
                        <value>2r</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <!-- APP是否在前台运行 -->
                    <argument>
                        <key>AdviewabilityForground</key>
                        <value>2s</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <!-- 可视视图可见性 -->
                    <argument>
                        <key>Adviewability</key>
                        <value>2f</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <!-- 可视视图可测量性 -->
                    <argument>
                        <key>AdMeasurability</key>
                        <value>2h</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <!-- 采集属性 end -->

                    <!-- 配置属性 start -->
                    <!-- 满足可视覆盖率 -->
                    <argument>
                        <key>AdviewabilityConfigArea</key>
                        <value>2v</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <!-- 满足可视时长 -->
                    <argument>
                        <key>AdviewabilityConfigThreshold</key>
                        <value>2u</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <!-- 可视视频播放时长 -->
                    <argument>
                        <key>AdviewabilityVideoDuration</key>
                        <value>2w</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <!-- 可视视频播放进度监测事件类型 -->
                    <argument>
                        <key>AdviewabilityVideoProgress</key>
                        <value>2a</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <!-- 可视监测传入的视频播放类型 -->
                    <argument>
                        <key>AdviewabilityVideoPlayType</key>
                        <value>1g</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <!-- 视频可视化播放进度配置依次为:25%50%75%100% -->
                    <argument>
                        <key>AdviewabilityVideoProgressPoint</key>
                        <value>2x</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <!-- 配置属性 end -->

                    <!-- 控制属性 start -->
                    <!-- 是否开启ViewAbility监测 -->
                    <!--
                    <argument>
                        <key>AdviewabilityEnable</key>
                        <value>2p</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    -->
                    <!-- 可视轨迹数据是否上报 -->
                   <!--
                    <argument>
                        <key>AdviewabilityRecord</key>
                        <value>va</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                   -->
                    <!-- 控制属性 end -->

                </viewabilityarguments>
            </config>
            <!-- <separator>&amp;</separator> -->
            <separator>,</separator>
            <!-- <equalizer>=</equalizer> -->
            <equalizer></equalizer>
            <!--如果设置true   timeStamper使用秒-->
            <timeStampUseSecond>true</timeStampUseSecond>
        </company>
