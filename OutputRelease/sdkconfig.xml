        <length>0</length>
        <queueExpirationSecs>60</queueExpirationSecs>
        <!--发送超时时间-->
        <timeout>60</timeout>
    </offlineCache>
    
    <viewability>
        <!--viewability监测的时间间隔（ms）-->
        <intervalTime>100</intervalTime>
